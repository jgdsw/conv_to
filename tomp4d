find . -name '*.flv' -type f -print0 | sort -z | xargs -0 -n 1 conv_to.py -d 
find . -name '*.mpg' -type f -print0 | sort -z | xargs -0 -n 1 conv_to.py -d
find . -name '*.avi' -type f -print0 | sort -z | xargs -0 -n 1 conv_to.py -d
find . -name '*.mkv' -type f -print0 | sort -z | xargs -0 -n 1 conv_to.py -d
